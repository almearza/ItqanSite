                @Html.DisplayFor(model => model.Lang1.Name_lang)
            </td>

        </tr>
        
        
        <tr>
            <td>
                <a href="~/Contact_Withme/Index" class="btn btn-primary" style="max-width:150px;text-align:center;display:block"> رجوع</a>
            </td>

            @*@Html.ActionLink("Edit", "Edit", new { id = Model.ID_contact })*@ 

