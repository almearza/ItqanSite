            <td class="col-md-10">
                @Html.DisplayFor(model => model.Date)
            </td>

        </tr>
        <tr>
            <th class="col-md-2"> </th>
            <td class="col-md-10"></td>

        </tr>
        <tr>
            <th class="col-md-2"> اللغة</th>
