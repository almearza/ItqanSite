}



    <table class="table container text-center">
        <tr>
            <th class="col-md-2">البريد الالكتروني </th>
            <td class="col-md-10">
                @Html.DisplayFor(model => model.Email)
            </td>

        </tr>
        <tr>
            <th class="col-md-2"> الاسم</th>
            <td class="col-md-10">
                @Html.DisplayFor(model => model.FullName)
            </td>

        </tr>
        <tr>
            <th class="col-md-2"> الجوال</th>
